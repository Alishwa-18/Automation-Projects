    {
      "parameters": {
        "jsCode": "const item = $input.item.json;\n\n// --- Core Fields ---\nconst meetingId = item.id;\nconst subject = item.subject || 'Untitled Meeting';\n\n// --- Time Handling ---\nconst start = item.start?.dateTime || item.start;\nconst end = item.end?.dateTime || item.end;\nconst startTime = new Date(start);\nconst endTime = new Date(end);\nconst now = new Date();\n\nconst hoursUntilMeeting = (startTime - now) / 36e5;\nconst daysUntilMeeting = +(hoursUntilMeeting / 24).toFixed(2);\n\n// --- Location / Link ---\nconst meetingLink = item.onlineMeetingUrl || item.webLink || item.location?.displayName || 'No link';\nconst location = item.location?.displayName || meetingLink;\n\n// --- Organizer Info ---\nconst organizerName = item.organizer?.emailAddress?.name || 'Unknown';\nconst organizerEmail = item.organizer?.emailAddress?.address || '';\n\n// --- Description & Body ---\nconst description = item.bodyPreview || item.body?.content || '';\n\n// --- Attendees ---\nconst attendees = (item.attendees || []).map(a => ({\n  name: a.emailAddress?.name || 'Unknown',\n  email: a.emailAddress?.address || '',\n  status: a.status?.response || 'none'\n}));\n\n// --- Custom Reminder Keyword ---\nlet customReminderHours = null;\nconst reminderMatch = description.match(/reminder:\\s*(\\d+)\\s*hours?/i);\nif (reminderMatch) customReminderHours = parseInt(reminderMatch[1]);\n\n// --- Human-Readable Time ---\nconst startTimeFormatted = startTime.toLocaleString('en-US', { \n  timeZone: 'Asia/Karachi', // change to your region if needed\n  weekday: 'long', month: 'long', day: 'numeric',\n  hour: '2-digit', minute: '2-digit', timeZoneName: 'short'\n});\n\n// --- Reminder Triggers (flags) ---\n// These are used by ‚ÄúDecide Reminder Schedule‚Äù node later\nconst shouldTrigger7d  = hoursUntilMeeting <= 168 && hoursUntilMeeting > 160; // between 6.6‚Äì7 days\nconst shouldTrigger24h = hoursUntilMeeting <= 24 && hoursUntilMeeting > 20;   // between 20‚Äì24h\nconst shouldTrigger1h  = hoursUntilMeeting <= 1.5 && hoursUntilMeeting > 0.5; // between 30‚Äì90min\n\n// --- Final JSON output ---\nreturn [{\n  json: {\n    meetingId,\n    subject,\n    description,\n    startTime: startTime.toISOString(),\n    endTime: endTime.toISOString(),\n    startTimeFormatted,\n    location,\n    meetingLink,\n    organizerName,\n    organizerEmail,\n    attendees,\n    attendeeEmails: attendees.map(a => a.email),\n    attendeeNames: attendees.map(a => a.name),\n    attendeeCount: attendees.length,\n    hoursUntilMeeting: +hoursUntilMeeting.toFixed(2),\n    daysUntilMeeting,\n    customReminderHours,\n    shouldTrigger7d,\n    shouldTrigger24h,\n    shouldTrigger1h,\n    isCancelled: !!item.isCancelled,\n    lastModifiedDateTime: item.lastModifiedDateTime || item.lastModifiedDate,\n    rawEvent: item\n  }\n}];\n"
      },
