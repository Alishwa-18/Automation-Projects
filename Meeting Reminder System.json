    {
      "parameters": {
        "jsCode": "// Get the meeting data from the PREVIOUS node\nlet meeting;\ntry {\n  meeting = $('Extract & Parse Meeting Data').item.json;\n} catch (e) {\n  console.log('Could not find Extract & Parse Meeting Data node by name, using previous node');\n  const allItems = $input.all();\n  if (allItems.length > 0) {\n    meeting = allItems[0].json;\n  } else {\n    throw new Error('No meeting data found');\n  }\n}\n\n// Get Notion search results (could be empty)\nconst notionResults = $input.all();\n\n// Default values (non-project meeting)\nlet meetingType = 'General Meeting';\nlet projectId = null;\nlet projectName = null;\nlet projectManager = null;\nlet projectManagerEmail = null;\nlet clientName = null;\nlet projectStatus = null;\nlet notionUrl = null;\n\n// üÜï add this\nlet notionPageId = null;   // store the Notion page ID\n\n// ============================================\n// SECTION 1: Handle Empty Notion Results\n// ============================================\nconsole.log(`Notion search returned ${notionResults.length} results`);\n\nif (notionResults.length > 0 && notionResults[0].json) {\n  console.log('Project found in Notion via bodyPreview search - processing...');\n  \n  const project = notionResults[0].json;\n  meetingType = 'Project Meeting';\n  projectId = project.id;\n  notionUrl = project.url || null;\n  \n  // üÜï capture Notion page ID (same as project.id)\n  notionPageId = project.id || null;\n\n  // Extract project info\n  if (project.properties) {\n    // Project Name\n    if (project.properties['Project Name']?.title?.[0]?.plain_text) {\n      projectName = project.properties['Project Name'].title[0].plain_text;\n    } else if (project.properties['Name']?.title?.[0]?.plain_text) {\n      projectName = project.properties['Name'].title[0].plain_text;\n    } else if (project.properties['Title']?.title?.[0]?.plain_text) {\n      projectName = project.properties['Title'].title[0].plain_text;\n    }\n\n    // Client Name\n    if (project.properties['Client']?.rich_text?.[0]?.plain_text) {\n      clientName = project.properties['Client'].rich_text[0].plain_text;\n    } else if (project.properties['Client Name']?.rich_text?.[0]?.plain_text) {\n      clientName = project.properties['Client Name'].rich_text[0].plain_text;\n    } else if (project.properties['Customer']?.rich_text?.[0]?.plain_text) {\n      clientName = project.properties['Customer'].rich_text[0].plain_text;\n    } else if (project.properties['Client']?.select?.name) {\n      clientName = project.properties['Client'].select.name;\n    }\n\n    // Project Status\n    if (project.properties['Status']?.select?.name) {\n      projectStatus = project.properties['Status'].select.name;\n    } else if (project.properties['Status']?.status?.name) {\n      projectStatus = project.properties['Status'].status.name;\n    } else if (project.properties['Project Status']?.select?.name) {\n      projectStatus = project.properties['Project Status'].select.name;\n    }\n\n    // Project Manager\n    if (project.properties['Project Manager']?.people?.[0]) {\n      projectManager = project.properties['Project Manager'].people[0].name;\n      projectManagerEmail = project.properties['Project Manager'].people[0].person?.email || null;\n    } else if (project.properties['PM']?.people?.[0]) {\n      projectManager = project.properties['PM'].people[0].name;\n      projectManagerEmail = project.properties['PM'].people[0].person?.email || null;\n    } else if (project.properties['Assigned To']?.people?.[0]) {\n      projectManager = project.properties['Assigned To'].people[0].name;\n      projectManagerEmail = project.properties['Assigned To'].people[0].person?.email || null;\n    } else if (project.properties['Owner']?.people?.[0]) {\n      projectManager = project.properties['Owner'].people[0].name;\n      projectManagerEmail = project.properties['Owner'].people[0].person?.email || null;\n    } else if (project.properties['Manager']?.people?.[0]) {\n      projectManager = project.properties['Manager'].people[0].name;\n      projectManagerEmail = project.properties['Manager'].people[0].person?.email || null;\n    } else if (project.properties['Project Manager Email']?.email) {\n      projectManagerEmail = project.properties['Project Manager Email'].email;\n    } else if (project.properties['PM Email']?.email) {\n      projectManagerEmail = project.properties['PM Email'].email;\n    }\n  }\n\n  console.log(`Project Details: ${projectName} | Client: ${clientName} | PM: ${projectManager}`);\n} else {\n  console.log('No matching project found in Notion');\n}\n\n// ============================================\n// SECTION 3: Fallback - Check Meeting Attendees for PM\n// ============================================\nif (meetingType === 'Project Meeting' && !projectManagerEmail && meeting.attendees && meeting.attendees.length > 0) {\n  console.log('No PM in Notion - checking meeting attendees...');\n  \n  const pmAttendee = meeting.attendees.find(a => {\n    const nameLower = (a.name || '').toLowerCase();\n    const emailLower = (a.email || '').toLowerCase();\n    \n    return nameLower.includes('project manager') ||\n           nameLower.includes(' pm ') ||\n           nameLower === 'pm' ||\n           emailLower.includes('pm@') ||\n           emailLower.includes('projectmanager') ||\n           nameLower.includes('manager');\n  });\n  \n  if (pmAttendee) {\n    projectManager = pmAttendee.name;\n    projectManagerEmail = pmAttendee.email;\n    console.log(`Found PM in attendees: ${projectManager}`);\n  }\n}\n\n// ============================================\n// SECTION 4: Check for Personal Meeting\n// ============================================\nconst personalKeywords = [\n  'personal', 'dentist', 'doctor', 'appointment',\n  'lunch', 'coffee', 'break', 'vacation', 'holiday',\n  'gym', 'workout', 'family', 'birthday', 'anniversary'\n];\n\nconst subjectLower = (meeting.subject || '').toLowerCase();\nconst descriptionLower = (meeting.description || '').toLowerCase();\nconst bodyPreviewLower = meeting.rawEvent?.bodyPreview ? meeting.rawEvent.bodyPreview.toLowerCase() : '';\n\nconst hasPersonalKeyword = personalKeywords.some(keyword =>\n  subjectLower.includes(keyword) ||\n  descriptionLower.includes(keyword) ||\n  bodyPreviewLower.includes(keyword)\n);\n\nif (hasPersonalKeyword) {\n  meetingType = 'Personal';\n}\n\n// ============================================\n// SECTION 5: Return Complete Meeting Data\n// ============================================\nreturn [{\n  json: {\n    meetingId: meeting.meetingId,\n    subject: meeting.subject,\n    startTime: meeting.startTime,\n    endTime: meeting.endTime,\n    startTimeFormatted: meeting.startTimeFormatted,\n    location: meeting.location,\n    organizer: meeting.organizer,\n    organizerEmail: meeting.organizerEmail,\n    description: meeting.description,\n    bodyPreview: meeting.rawEvent?.bodyPreview || '',\n    attendees: meeting.attendees || [],\n    attendeeEmails: meeting.attendeeEmails || [],\n    attendeeNames: meeting.attendeeNames || [],\n    attendeeCount: meeting.attendeeCount || 0,\n    hoursUntilMeeting: meeting.hoursUntilMeeting,\n    daysUntilMeeting: meeting.daysUntilMeeting,\n    customReminderHours: meeting.customReminderHours,\n    meetingType: meetingType,\n    isProjectMeeting: meetingType === 'Project Meeting',\n    isPersonalMeeting: meetingType === 'Personal',\n    isGeneralMeeting: meetingType === 'General Meeting',\n    projectId: projectId,\n    projectName: projectName,\n    projectManager: projectManager,\n    projectManagerEmail: projectManagerEmail,\n    clientName: clientName,\n    projectStatus: projectStatus,\n    notionUrl: notionUrl,\n    // üÜï include the Notion page ID for downstream subflows\n    notionPageId: notionPageId,\n    hasProjectManager: projectManagerEmail !== null && projectManagerEmail !== ''\n  }\n}];\n\n"
      },
