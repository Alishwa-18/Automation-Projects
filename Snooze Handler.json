      ],
      "id": "ea411baa-ecfa-4720-8696-2ecc5e50a044",
      "name": "Extract snoozeminutes"
    },
    {
      "parameters": {
        "jsCode": "// Handle Telegram callback data safely\nlet cb;\n\n// Handle both possible payload shapes (Telegram â†’ n8n)\nif ($json.callback_query && $json.callback_query.data) {\n  cb = $json.callback_query.data;\n} else if ($json.data && $json.data.callback_query && $json.data.callback_query.data) {\n  cb = $json.data.callback_query.data;\n} else {\n  throw new Error(\"No callback data found in input\");\n}\n\n// Split action and meeting/project name\nconst [action, ...rest] = cb.split(':');\nconst notionid = rest.join(':').trim(); // supports names with colons\n\nreturn [\n  {\n    json: {\n      action,\n      notionid\n    }\n  }\n];\n\n"
      },
